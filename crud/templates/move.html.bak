{% extends 'layout.html' %}
{% block body %}
  <div class='row'>
    <div class='col-5 mx-auto'>
      <h3>Add Product</h3><hr>
      <form method='post' action='{{url_for("move")}}'>
        <div class='form-group'>
          <label>Product Name</label>
          <input type='text' name='pnamee' required class='form-control'>
        </div>
		<div class='form-group'>
          <label>Product_id</label>
          <input type='text' name='pidd' required class='form-control'>
        </div>
		<div class='form-group'>
          <label>Quantity</label>
          <input type='text' name='quantityy' required class='form-control'>
        </div>
        <input type='submit' value='Submit' class='btn btn-primary'>
      </form>
    
    </div>
  </div>
{% endblock %}